#include "stack.h"

void stack_init()
{
	stack.last_element = 0;
}

void stack_push(Position element)
{
	stack.space[last_element++] = element;
}

Position stack_pop()
{
	if (stack.last_element > 0)
	{
		return stack.space[stack.last_element--];
	}
	else
	{
		Position tmp;
		tmp.x = -1;
		tmp.y = -1;
		
		return tmp;
	}	
}

unsigned char stack_length()
{
	return stack.last_element;
}